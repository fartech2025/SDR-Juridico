{
  "version": 2,
  "buildCommand": "cd \"Sdr juridico\" && npm run build",
  "outputDirectory": "Sdr juridico/dist",
  "installCommand": "cd \"Sdr juridico\" && npm install",
  "framework": "vite",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets/).*)",
      "destination": "/index.html"
    }
  ]
}